{
  "name": "@leeovery/claude-manager",
  "version": "0.0.0-development",
  "packageManager": "pnpm@9.15.4",
  "description": "Plugin manager for Claude Code skills and commands",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "claude-plugins": "dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "postinstall": "[ -f dist/postinstall.js ] && node dist/postinstall.js || true",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "claude",
    "claude-code",
    "plugin",
    "skills",
    "commands"
  ],
  "author": "Lee Overy <me@leeovery.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leeovery/claude-manager.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.16"
  }
}
